{
  "name": "mediaManager",
  "displayName": "Media manager",
  "publisher": "AlessandroMurtas",
  "icon": "logo.png",
  "os": ["Windows"],
  "description": "This extension lets you control every media player (Spotify, WMP, MediaMonkey...) and show media info on your status bar",
  "version": "1.0.4",
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./build/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mediaManager.pause",
        "title": "Pause"
      },
      {
        "command": "mediaManager.previousMedia",
        "title": "Previous Media"
      },
      {
        "command": "mediaManager.nextMedia",
        "title": "Next Media"
      }
    ],
    "configuration": {
      "title": "Media manager",
      "properties": {}
    }
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "repository": {
    "type": "GitHub",
    "url": "https://github.com/trimone9000/vscode-mediaManager"
  }
}
