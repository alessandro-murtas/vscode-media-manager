{
  "name": "media-manager",
  "displayName": "Media manager",
  "publisher": "AlessandroMurtas",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./build/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "media-manager.pause",
        "title": "Pause"
      },
      {
        "command": "media-manager.previousMedia",
        "title": "Previous Media"
      },
      {
        "command": "media-manager.nextMedia",
        "title": "Next Media"
      }
    ],
    "configuration": {
      "title": "Media manager",
      "properties": {}
    }
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "repository": {
    "type": "GitHub",
    "url": "https://github.com/trimone9000/vscode-media-manager"
  }
}
